<br>
<div class="col-md-6">
 <div class="card">
  <div class="card-body">


 <%- model_class = Profile -%>
 <div class="page-header">
   <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
 </div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:screenname) %>:</strong></dt>
  <dd><%= @profile.screenname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:city) %>:</strong></dt>
  <dd><%= @profile.city %></dd>
</dl>

 <%= link_to t('.back', :default => t("helpers.links.back")),
              profiles_path, :class => 'btn btn-primary'  %>
 <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_profile_path(@profile), :class => 'btn btn-primary' %>
 <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              profile_path(@profile),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>

</div>
  </div>
</div>
